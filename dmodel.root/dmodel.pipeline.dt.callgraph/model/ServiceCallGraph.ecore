  <eClassifiers xsi:type="ecore:EClass" name="ServiceCallGraph">
    <eOperations name="addNode" eType="#//ServiceCallGraphNode">
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="body" value="ServiceCallGraphNode node = ServiceCallGraphFactory.eINSTANCE.createServiceCallGraphNode();&#xA;node.setHost(host);&#xA;node.setSeff(seff);&#xA;getNodes().add(node);&#xA;return node;"/>
      </eAnnotations>
